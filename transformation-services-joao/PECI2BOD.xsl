                                                    <xsl:with-param name="xpath">
                                                        <xsl:for-each select="peci:Distribution_Percentage">
                                                            <xsl:call-template name="print-field-xpath"/>            
                                                        </xsl:for-each>   
                                                    </xsl:with-param>
                                                </xsl:call-template>   
