                <xsl:when test="$GenderCode = 'Male'">
                    <xsl:value-of select="'Male'"/>
                </xsl:when>
                <xsl:when test="$GenderCode = 'Female'">
                    <xsl:value-of select="'Female'"/>
                </xsl:when>
                <xsl:when test="$GenderCode = 'Not_Declared'">
