                                    <xsl:with-param name="Country">
                                        <xsl:choose>                          
                                            <xsl:when test="string-length(peci:Country) > 0">
                                                <xsl:value-of select="peci:Country"/>
                                            </xsl:when>
                                            <xsl:otherwise>
                                                <xsl:value-of select="'*'"/>
                                            </xsl:otherwise>
                                        </xsl:choose>
