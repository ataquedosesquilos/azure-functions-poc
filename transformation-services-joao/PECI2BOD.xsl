                                </xsl:variable>
                                <xsl:call-template name="usedPECIData">
                                    <xsl:with-param name="xpath" select="$nodePath">                                                               
                                    </xsl:with-param>
                                </xsl:call-template> 
                            </hr:Percentage>
                        </nga:PaymentPercentage>
