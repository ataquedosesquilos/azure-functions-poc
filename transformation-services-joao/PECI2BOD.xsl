                                                            <xsl:for-each select="peci:Position/ptdf:ScheduledDays">
                                                                <xsl:call-template name="print-field-xpath"/>            
                                                            </xsl:for-each>   
                                                        </xsl:with-param>
                                                    </xsl:call-template>  
