                                <xsl:call-template name="usedPECIData">
                                    <xsl:with-param name="xpath">
                                        <xsl:for-each select="peci:Legal_Name[@*[local-name()]]/peci:Social_Suffix[@*[local-name() != 'isDeleted']]">
