                    <xsl:if test="starts-with(nga:Payment_Method, 'BANK')">
                        <nga:DirectDepositAccount>
                            <xsl:if test="string-length(peci:Account_Type)>0">
                                <xsl:variable name="typeCode">
                                    <xsl:choose>
                                        <xsl:when test="peci:Account_Type = 'DDA'">
