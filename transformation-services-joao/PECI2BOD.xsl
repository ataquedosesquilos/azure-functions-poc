                                    <xsl:if test="(peci:Personal[string-length(@*[local-name()]) = 0] or string-length(peci:Personal) = 0) and ($checkHIR-RwithHIR = 'N' or ($checkHIR-RwithHIR = 'Y' and ../peci:Derived_Event_Code = 'DTA'))">
