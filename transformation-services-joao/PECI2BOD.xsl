                                                    <xsl:call-template name="usedPECIData">
                                                        <xsl:with-param name="xpath">
                                                            <xsl:for-each select="peci:Position[not(peci:Position_End_Date)]/peci:Work_Shift[@*[local-name() != 'isDeleted'] or string-length(@*[local-name()]) = 0]">
